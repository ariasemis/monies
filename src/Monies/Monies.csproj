<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Arias Emilio</Authors>
    <Description>Lossless monetary operations and multi-currency support</Description>
    <PackageProjectUrl>https://github.com/ariasemis/monies</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ariasemis/monies</RepositoryUrl>
    <PackageTags>money currency</PackageTags>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>
  <Import Project="..\.paket\Paket.Restore.targets" />
</Project>